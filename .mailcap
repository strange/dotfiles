image/jpg; open -a Preview %s; jametemplate=%s.jpg
image/jpeg; open -a Preview %s; needsterminal; nametemplate=%s.jpeg
image/gif; open -a Preview %s; needsterminal; nametemplate=%s.gif
image/png; open -a Preview %s; needsterminal; nametemplate=%s.png
video/*; open -a VLC %s
application/pdf; open -a Preview %s; needsterminal; nametemplate=%s.pdf

application/zip; mkdir -p ~/Downloads/mutt && cp %s ~/Downloads/mutt/$(basename %s)

application/rtf; open %s
application/msword; open %s

application/octet-stream; open %s

text/html; lynx -force-html %s
