image/jpg; ~/.mutt/bin/open %s jpg
image/jpeg; ~/.mutt/bin/open %s jpg
image/pjpeg; ~/.mutt/bin/open %s jpg
image/png; ~/.mutt/bin/open %s png
image/gif; ~/.mutt/bin/open %s gif
image/psd; ~/.mutt/bin/open %s psd

application/photoshop; ~/.mutt/bin/open %s psd
application/zip; ~/.mutt/bin/open %s zip
application/pdf; ~/.mutt/bin/open %s pdf

application/msword; ~/.mutt/bin/open %s doc
application/vnd.openxmlformats-officedocument.wordprocessingml.document; ~/.mutt/bin/open %s docx
application/vnd.ms-excel; ~/mutt/bin/open %s xls
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; ~/mutt/bin/open %s xlsx
application/vnd.ms-powerpoint; ~/mutt/bin/open %s ppt

text/html; w3m -I %{charset} -T text/html; copiousoutput;

application/octet-stream; ~/.mutt/bin/open %s "-"

