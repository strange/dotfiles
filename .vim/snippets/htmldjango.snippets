snippet load
	{% load ${1} %}

snippet extends
	{% extends ${1} %}

snippet if
	{% if ${1} %}${3}{% endif %}

snippet endif
	{% endif %}

snippet ifequal
	{% ifequal ${1} %}
		${2}
	{% endifequal %}

snippet else
	{% else %}
	
snippet block
	{% block ${1} %}${2}{% endblock %}

snippet for
	{% for ${1} in ${2} %}${3}{% endfor %}

snippet endfor
	{% endfor %}

snippet url
	{% url ${1} %}
